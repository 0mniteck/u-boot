runcmd:
  - |
    for i in {1..15}; do
        echo 125 | sudo tee /sys/class/hwmon/hwmon3/pwm1
        sleep 5
        current_speed=$(cat /sys/class/hwmon/hwmon3/pwm1)
        if [ "$current_speed" -eq 125 ]; then
            echo "Fan speed set to 125"
            break
        fi
    done
  - ufw enable
  - apt update
  - apt install snapd gnupg2 cloudflare-warp -y
  - systemctl start warp-svc
  - warp-cli --accept-tos registration new
  - warp-cli --accept-tos mode warp+doh
  - warp-cli --accept-tos dns families malware
  - warp-cli --accept-tos connect
  - apt remove --purge ufw -y
  - snap install ufw
  - sleep 10s
  - ufw enable
  - echo run_done > /tmp/run
autoinstall:
  version: 1
  network:
    version: 2
    ethernets:
      end0:
        dhcp6: true
        dhcp4: true
  interactive-sections:
    - storage
  early-commands:
    - |
      for i in {1..15}; do
          echo 125 | sudo tee /sys/class/hwmon/hwmon3/pwm1
          sleep 5
          current_speed=$(cat /sys/class/hwmon/hwmon3/pwm1)
          if [ "$current_speed" -eq 125 ]; then
              echo "Fan speed set to 125"
              break
          fi
      done
    - echo early_done > /tmp/early
